<?php
namespace jamradio\applicationBundle\Display;
use jamradio\applicationBundle\Entity\User;
use jamradio\applicationBundle\Entity\Instrument;
use jamradio\applicationBundle\Repository\UserRepository;
use Symfony\Component\HttpFoundation\Session\Session;
use Symfony\Component\HttpFoundation\Request;

class DisplayMusiciens
{

  public function displayMusiciensOnLine()
  {
    $user = new User();
    $em = $this->getDoctrine()->getManager();
    $user = $em->getRepository('jamradioapplicationBundle:User')->findBy(array('online' => 1));
    return $user;
  }
}

?>
